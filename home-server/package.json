{
  "name": "home-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "lint": "eslint public/javascripts/src/*.js",
    "babel": "babel --source-maps true public/javascripts/src --out-dir public/javascripts/build",
    "babel:watch": "babel -w --source-maps true public/javascripts/src --out-dir public/javascripts/build",
    "build": "npm run babel",
    "build:watch": "npm run babel:watch"
  },
  "dependencies": {
    "body-parser": "^1.12.4",
    "cookie-parser": "^1.3.5",
    "debug": "^2.2.0",
    "express": "^4.12.4",
    "jade": "^1.10.0",
    "morgan": "^1.5.3",
    "serve-favicon": "^2.2.1"
  }
}
